    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#availableIn -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#availableIn">
        <rdfs:domain rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#City"/>
        <rdfs:comment>Relates a service where the place where you can use it</rdfs:comment>
        <rdfs:label>availableIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#locatedIn -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#locatedIn">
        <rdfs:domain rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Place"/>
        <rdfs:comment>Relates an entity to the place or geographical area where it is situated</rdfs:comment>
        <rdfs:label>locatedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#occursIn -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#occursIn">
        <rdfs:domain rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#City"/>
        <rdfs:comment>Relates an event or activity to the place where it takes place</rdfs:comment>
        <rdfs:label>occursIn</rdfs:label>
    </owl:ObjectProperty>
    


