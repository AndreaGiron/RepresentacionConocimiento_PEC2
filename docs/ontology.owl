    <rdf:Description rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/score">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/stars">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/r">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/p">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment>If a hotel has four o more stars and its score is greater than 4, then is a recommended hotel</rdfs:comment>
        <rdfs:label>RecommendedHotelRule</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThanOrEqual"/>
                        <swrl:arguments>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/stars"/>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </swrl:arguments>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasStarRating"/>
                                <swrl:argument1 rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/h"/>
                                <swrl:argument2 rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/stars"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasHotelScore"/>
                                        <swrl:argument1 rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/h"/>
                                        <swrl:argument2 rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/score"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
