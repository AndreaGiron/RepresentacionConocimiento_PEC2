    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Beach -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Beach">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction"/>
        <rdfs:comment>A sandy or pebbly area beside a body of water, popular for recreation</rdfs:comment>
        <rdfs:label>Beach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bike -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bike">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Transport"/>
        <rdfs:comment>A human-powered vehicle with two wheels, used for individual urban or recreational transport</rdfs:comment>
        <rdfs:label>Bike</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/Bicycle</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bus -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bus">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Transport"/>
        <rdfs:comment>A large motor vehicle that carries passengers by road, typically along a fixed route</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#City -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#City">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Settlement"/>
        <rdfs:comment>Urban area with a high population density and significant infrastructure and services</rdfs:comment>
        <rdfs:label>City</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#CulturalAttraction -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#CulturalAttraction">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction"/>
        <rdfs:comment>A tourist site of cultural significance, such as theaters, heritage sites, or historical buildings</rdfs:comment>
        <rdfs:label>CulturalAttraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Event -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Event">
        <rdfs:comment>An occurrence such as a concert, festival, or exhibition that attracts tourists or locals</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#FoodEstablishment -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#FoodEstablishment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Place"/>
        <rdfs:comment>A place where food is prepared and served to the public, including restaurants, cafes, bars, or food stands</rdfs:comment>
        <rdfs:label>FoodEstablishment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Hotel -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Hotel">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAsset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasHotelPrice"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">0</xsd:minExclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasStarRating"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction"/>
        <rdfs:comment>A commercial establishment providing lodging, meals, and other guest services</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/Hotel</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Monument -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Monument">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction"/>
        <rdfs:comment>A structure or statue built to commemorate a notable person or event</rdfs:comment>
        <rdfs:label>Monument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Museum -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Museum">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction"/>
