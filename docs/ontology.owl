    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Beach -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Beach">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction"/>
        <rdfs:comment>A sandy or pebbly area beside a body of water, popular for recreation</rdfs:comment>
        <rdfs:label>Beach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bike -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bike">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Transport"/>
        <rdfs:comment>A human-powered vehicle with two wheels, used for individual urban or recreational transport</rdfs:comment>
        <rdfs:label>Bike</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/Bicycle</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bus -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bus">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Transport"/>
        <rdfs:comment>A large motor vehicle that carries passengers by road, typically along a fixed route</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#City -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#City">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Settlement"/>
        <rdfs:comment>Urban area with a high population density and significant infrastructure and services</rdfs:comment>
        <rdfs:label>City</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#CulturalAttraction -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#CulturalAttraction">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction"/>
        <rdfs:comment>A tourist site of cultural significance, such as theaters, heritage sites, or historical buildings</rdfs:comment>
        <rdfs:label>CulturalAttraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Event -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Event">
        <rdfs:comment>An occurrence such as a concert, festival, or exhibition that attracts tourists or locals</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#FoodEstablishment -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#FoodEstablishment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Place"/>
        <rdfs:comment>A place where food is prepared and served to the public, including restaurants, cafes, bars, or food stands</rdfs:comment>
        <rdfs:label>FoodEstablishment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Hotel -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Hotel">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAsset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasHotelPrice"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">0</xsd:minExclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasStarRating"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction"/>
        <rdfs:comment>A commercial establishment providing lodging, meals, and other guest services</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/Hotel</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Monument -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Monument">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction"/>
        <rdfs:comment>A structure or statue built to commemorate a notable person or event</rdfs:comment>
        <rdfs:label>Monument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Museum -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Museum">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction"/>
        <rdfs:comment>An institution that conserves and displays objects of artistic, cultural, or scientific interest</rdfs:comment>
        <rdfs:label>Museum</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/Museum</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Park -->


    <owl:Class rdf:about="http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Park">
