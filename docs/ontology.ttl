###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasTransportID
:hasTransportID rdf:type owl:DatatypeProperty ;
                rdfs:domain :Transport ;
                rdfs:range xsd:int .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasTransportPrice
:hasTransportPrice rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Transport ;
                   rdfs:range xsd:decimal .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#isRecomended
:isRecomended rdf:type owl:DatatypeProperty ;
              rdfs:domain :Hotel ;
              rdfs:range xsd:boolean .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :TouristAttraction ;
       rdfs:comment "A sandy or pebbly area beside a body of water, popular for recreation" ;
       rdfs:label "Beach" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bike
:Bike rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "A human-powered vehicle with two wheels, used for individual urban or recreational transport" ;
      rdfs:label "Bike" ;
      rdfs:seeAlso "https://dbpedia.org/page/Bicycle" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "A large motor vehicle that carries passengers by road, typically along a fixed route" ;
     rdfs:label "Bus" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Settlement ;
      rdfs:comment "Urban area with a high population density and significant infrastructure and services" ;
      rdfs:label "City" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#CulturalAttraction
:CulturalAttraction rdf:type owl:Class ;
                    rdfs:subClassOf :TouristAttraction ;
                    rdfs:comment "A tourist site of cultural significance, such as theaters, heritage sites, or historical buildings" ;
                    rdfs:label "CulturalAttraction" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Event
:Event rdf:type owl:Class ;
       rdfs:comment "An occurrence such as a concert, festival, or exhibition that attracts tourists or locals" ;
       rdfs:label "Event" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#FoodEstablishment
:FoodEstablishment rdf:type owl:Class ;
                   rdfs:subClassOf :Place ;
                   rdfs:comment "A place where food is prepared and served to the public, including restaurants, cafes, bars, or food stands" ;
                   rdfs:label "FoodEstablishment" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :TouristAsset ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasHotelPrice ;
                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                              owl:onDatatype xsd:int ;
                                              owl:withRestrictions ( [ xsd:minExclusive "0"^^xsd:int
                                                                     ]
                                                                   )
                                            ]
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasStarRating ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:int
                       ] ;
       owl:disjointWith :TouristAttraction ;
       rdfs:comment "A commercial establishment providing lodging, meals, and other guest services" ;
       rdfs:label "Hotel" ;
       rdfs:seeAlso "https://dbpedia.org/page/Hotel" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :TouristAttraction ;
          rdfs:comment "A structure or statue built to commemorate a notable person or event" ;
          rdfs:label "Monument" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :TouristAttraction ;
        rdfs:comment "An institution that conserves and displays objects of artistic, cultural, or scientific interest" ;
        rdfs:label "Museum" ;
        rdfs:seeAlso "https://dbpedia.org/page/Museum" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :TouristAttraction ;
      rdfs:comment "A public green area in a town, used for recreation and relaxation" ;
      rdfs:label "Park" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Place
:Place rdf:type owl:Class ;
       rdfs:comment "A general location or area of interest that can be visited" ;
       rdfs:label "Place" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf :FoodEstablishment ;
            rdfs:comment "An establishment that prepares and serves food and drinks to customers" ;
            rdfs:label "Restaurant" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Settlement
:Settlement rdf:type owl:Class ;
            rdfs:subClassOf :Place ;
            rdfs:comment "A human settlement, such as a village, town, or small community, usually smaller than a city" ;
            rdfs:label "Settlement" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Subway
:Subway rdf:type owl:Class ;
        rdfs:subClassOf :Transport ;
        rdfs:comment "An underground railway system used for urban passenger transport" ;
        rdfs:label "Subway" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Taxi
:Taxi rdf:type owl:Class ;
      rdfs:subClassOf :Transport .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAsset
:TouristAsset rdf:type owl:Class ;
              rdfs:subClassOf :Place ;
              rdfs:comment "A general resource or element of interest within the tourism domain, such as attractions, accommodations, or experiences" ;
              rdfs:label "TouristAsset" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#TouristAttraction
:TouristAttraction rdf:type owl:Class ;
                   rdfs:subClassOf :TouristAsset ;
                   rdfs:comment "A place of interest that draws visitors due to its cultural, historical, or natural value" ;
                   rdfs:label "TouristAttraction" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Tram
:Tram rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "A rail vehicle that runs on tracks along public urban streets, used for local transport" ;
      rdfs:label "Tram" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#Transport
:Transport rdf:type owl:Class ;
           rdfs:comment "Vehicles or systems used for moving from one place to another" ;
           rdfs:label "Transport" .


