@prefix : <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#> .

<http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0> rdf:type owl:Ontology ;
                                                                   owl:versionIRI <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0> ;
                                                                   rdfs:comment "Esta ontología tiene como objetivo modelar el dominio del turismo inteligente, abarcando la representación formal de eventos turísticos, oferta hotelera, atracciones, y restaurantes disponibles en diferentes ciudades. Su diseño permitirá estructurar, analizar y reutilizar la información de interés turístico para facilitar la recomendación de destinos y servicios personalizados a los usuarios. La ontología incluirá conceptos clave del ámbito turístico, así como relaciones relevantes entre ellos, permitiendo también la inferencia de conocimiento a partir de los datos disponibles" ;
                                                                   rdfs:isDefinedBy "Andrea Giron" ;
                                                                   rdfs:label "Tourism and recomendations" ;
                                                                   owl:versionInfo "v1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#title
rdfs:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#availableIn
:availableIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Transport ;
             rdfs:range :City ;
             rdfs:comment "Relates a service where the place where you can use it" ;
             rdfs:label "availableIn" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Place ;
           rdfs:range :Place ;
           rdfs:comment "Relates an entity to the place or geographical area where it is situated" ;
           rdfs:label "locatedIn" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#occursIn
:occursIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Event ;
          rdfs:range :City ;
          rdfs:comment "Relates an event or activity to the place where it takes place" ;
          rdfs:label "occursIn" .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasAttractionScore
:hasAttractionScore rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :TouristAttraction ;
                    rdfs:range xsd:decimal .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:string ;
         rdfs:comment "Indicates de Date when something happens" ;
         rdfs:label "hasDate" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasEventID
:hasEventID rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Event ;
            rdfs:range xsd:int .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasEventName
:hasEventName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:string ;
              rdfs:comment "Specifies the name or title of an entity" ;
              rdfs:label "hasEventName" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasEventType
:hasEventType rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasHotelPrice
:hasHotelPrice rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Indicates the monetary value or cost associated with an entity" ;
               rdfs:label "hasHotelPrice" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasHotelScore
:hasHotelScore rdf:type owl:DatatypeProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:decimal .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasHotelType
:hasHotelType rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Hotel ;
              rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasLocation
:hasLocation rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :City ;
             rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasPlaceID
:hasPlaceID rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Place ;
            rdfs:range xsd:int .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasPlaceName
:hasPlaceName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Place ;
              rdfs:range xsd:string ;
              rdfs:comment "Specifies the name or title of an entity" ;
              rdfs:label "hasPlaceName" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasPriceCategory
:hasPriceCategory rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Restaurant ;
                  rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasProvider
:hasProvider rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasRestaurantPrice
:hasRestaurantPrice rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Restaurant ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "Indicates the monetary value or cost associated with an entity" ;
                    rdfs:label "hasRestaurantPrice" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasRestaurantScore
:hasRestaurantScore rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Restaurant ;
                    rdfs:range xsd:decimal .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasServesCuisine
:hasServesCuisine rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Restaurant ;
                  rdfs:range xsd:string .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology/v1.0#hasStarRating
:hasStarRating rdf:type owl:DatatypeProperty ;
