@prefix : <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#> .

<http://www.w3.org/andrea/ontologies/2025/3/tourismOntology> rdf:type owl:Ontology ;
                                                              owl:versionIRI <http://www.w3.org/andrea/ontologies/2025/3/tourismOntology> ;
                                                              rdfs:isDefinedBy "Andrea Giron" ;
                                                              rdfs:label "Tourism and recomendations" ;
                                                              owl:versionInfo "v1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#title
rdfs:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#availableIn
:availableIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Transport ;
             rdfs:range :City ;
             rdfs:comment "Relates a service where the place where you can use it" ;
             rdfs:label "availableIn" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Place ;
           rdfs:range :Place ;
           rdfs:comment "Relates an entity to the place or geographical area where it is situated" ;
           rdfs:label "locatedIn" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#occursIn
:occursIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Event ;
          rdfs:range :City ;
          rdfs:comment "Relates an event or activity to the place where it takes place" ;
          rdfs:label "occursIn" .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasAttractionScore
:hasAttractionScore rdf:type owl:DatatypeProperty ;
                    rdfs:domain :TouristAttraction ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "Indicates de score of an entity" ;
                    rdfs:label "hasAttractionScore" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:string ;
         rdfs:comment "Indicates de Date when something happens" ;
         rdfs:label "hasDate" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasEventID
:hasEventID rdf:type owl:DatatypeProperty ;
            rdfs:domain :Event ;
            rdfs:range xsd:int ;
            rdfs:comment "Unique identifier" ;
            rdfs:label "hasEventID" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasEventName
:hasEventName rdf:type owl:DatatypeProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:string ;
              rdfs:comment "Specifies the name or title of an entity" ;
              rdfs:label "hasEventName" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasEventType
:hasEventType rdf:type owl:DatatypeProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:string ;
              rdfs:comment "Indicates which kind of event it is" ;
              rdfs:label "hasEventType" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasHotelPrice
:hasHotelPrice rdf:type owl:DatatypeProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Indicates the monetary value or cost associated with an entity" ;
               rdfs:label "hasHotelPrice" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasHotelScore
:hasHotelScore rdf:type owl:DatatypeProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Indicates de score of an entity" ;
               rdfs:label "hasHotelScore" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasHotelType
:hasHotelType rdf:type owl:DatatypeProperty ;
              rdfs:domain :Hotel ;
              rdfs:range xsd:string ;
              rdfs:comment "Indicates which kind of hotel it is" ;
              rdfs:label "hasHotelType" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasLocation
:hasLocation rdf:type owl:DatatypeProperty ;
             rdfs:domain :City ;
             rdfs:range xsd:string ;
             rdfs:comment "Indicates the place where the entity is" ;
             rdfs:label "hasLocation" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasPlaceID
:hasPlaceID rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Place ;
            rdfs:range xsd:int ;
            rdfs:comment "Unique identifier" ;
            rdfs:label "hasPlaceID" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasPlaceName
:hasPlaceName rdf:type owl:DatatypeProperty ;
              rdfs:domain :Place ;
              rdfs:range xsd:string ;
              rdfs:comment "Specifies the name or title of an entity" ;
              rdfs:label "hasPlaceName" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasPriceCategory
:hasPriceCategory rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Restaurant ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates the category based on the price" ;
                  rdfs:label "hasPriceCategory" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasProvider
:hasProvider rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:string ;
             rdfs:comment "Indicates the enterprise that provides the service" ;
             rdfs:label "hasProvider" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasRestaurantPrice
:hasRestaurantPrice rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Restaurant ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "Indicates the monetary value or cost associated with an entity" ;
                    rdfs:label "hasRestaurantPrice" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasRestaurantScore
:hasRestaurantScore rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Restaurant ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "Indicates de score of an entity" ;
                    rdfs:label "hasRestaurantScore" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasServesCuisine
:hasServesCuisine rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Restaurant ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates the type of food served" ;
                  rdfs:label "hasServedCuisine" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasStarRating
:hasStarRating rdf:type owl:DatatypeProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:int ;
               rdfs:comment "Indicates the number of stars" ;
               rdfs:label "hasStarRating" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasTransportID
:hasTransportID rdf:type owl:DatatypeProperty ;
                rdfs:domain :Transport ;
                rdfs:range xsd:int ;
                rdfs:comment "Unique identifier" ;
                rdfs:label "hasTransportID" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#hasTransportPrice
:hasTransportPrice rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Transport ;
                   rdfs:range xsd:decimal ;
                   rdfs:comment "Indicates the monetary value or cost associated with an entity" ;
                   rdfs:label "hasTransportPrice" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#isRecomended
:isRecomended rdf:type owl:DatatypeProperty ;
              rdfs:domain :Hotel ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Indicates if an entity is recomended or not based on its score and stars" ;
              rdfs:label "isRecommended" .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :TouristAttraction ;
       rdfs:comment "A sandy or pebbly area beside a body of water, popular for recreation" ;
       rdfs:label "Beach" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bike
:Bike rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "A human-powered vehicle with two wheels, used for individual urban or recreational transport" ;
      rdfs:label "Bike" ;
      rdfs:seeAlso "https://dbpedia.org/page/Bicycle" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "A large motor vehicle that carries passengers by road, typically along a fixed route" ;
     rdfs:label "Bus" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Settlement ;
      rdfs:comment "Urban area with a high population density and significant infrastructure and services" ;
      rdfs:label "City" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#CulturalAttraction
:CulturalAttraction rdf:type owl:Class ;
                    rdfs:subClassOf :TouristAttraction ;
                    rdfs:comment "A tourist site of cultural significance, such as theaters, heritage sites, or historical buildings" ;
                    rdfs:label "CulturalAttraction" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Event
:Event rdf:type owl:Class ;
       rdfs:comment "An occurrence such as a concert, festival, or exhibition that attracts tourists or locals" ;
       rdfs:label "Event" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#FoodEstablishment
:FoodEstablishment rdf:type owl:Class ;
                   rdfs:subClassOf :Place ;
                   rdfs:comment "A place where food is prepared and served to the public, including restaurants, cafes, bars, or food stands" ;
                   rdfs:label "FoodEstablishment" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :TouristAsset ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasHotelPrice ;
                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                              owl:onDatatype xsd:int ;
                                              owl:withRestrictions ( [ xsd:minExclusive "0"^^xsd:int
                                                                     ]
                                                                   )
                                            ]
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasStarRating ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:int
                       ] ;
       owl:disjointWith :TouristAttraction ;
       rdfs:comment "A commercial establishment providing lodging, meals, and other guest services" ;
       rdfs:label "Hotel" ;
       rdfs:seeAlso "https://dbpedia.org/page/Hotel" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :TouristAttraction ;
          rdfs:comment "A structure or statue built to commemorate a notable person or event" ;
          rdfs:label "Monument" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :TouristAttraction ;
        rdfs:comment "An institution that conserves and displays objects of artistic, cultural, or scientific interest" ;
        rdfs:label "Museum" ;
        rdfs:seeAlso "https://dbpedia.org/page/Museum" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :TouristAttraction ;
      rdfs:comment "A public green area in a town, used for recreation and relaxation" ;
      rdfs:label "Park" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Place
:Place rdf:type owl:Class ;
       rdfs:comment "A general location or area of interest that can be visited" ;
       rdfs:label "Place" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf :FoodEstablishment ;
            rdfs:comment "An establishment that prepares and serves food and drinks to customers" ;
            rdfs:label "Restaurant" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Settlement
:Settlement rdf:type owl:Class ;
            rdfs:subClassOf :Place ;
            rdfs:comment "A human settlement, such as a village, town, or small community, usually smaller than a city" ;
            rdfs:label "Settlement" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Subway
:Subway rdf:type owl:Class ;
        rdfs:subClassOf :Transport ;
        rdfs:comment "An underground railway system used for urban passenger transport" ;
        rdfs:label "Subway" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Taxi
:Taxi rdf:type owl:Class ;
      rdfs:subClassOf :Transport .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAsset
:TouristAsset rdf:type owl:Class ;
              rdfs:subClassOf :Place ;
              rdfs:comment "A general resource or element of interest within the tourism domain, such as attractions, accommodations, or experiences" ;
              rdfs:label "TouristAsset" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#TouristAttraction
:TouristAttraction rdf:type owl:Class ;
                   rdfs:subClassOf :TouristAsset ;
                   rdfs:comment "A place of interest that draws visitors due to its cultural, historical, or natural value" ;
                   rdfs:label "TouristAttraction" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Tram
:Tram rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "A rail vehicle that runs on tracks along public urban streets, used for local transport" ;
      rdfs:label "Tram" .


###  http://www.w3.org/andrea/ontologies/2025/3/tourismOntology#Transport
:Transport rdf:type owl:Class ;
           rdfs:comment "Vehicles or systems used for moving from one place to another" ;
           rdfs:label "Transport" .


